<div class='row'>
  <div class='col-lg-4'>
    <h4 class="pirate-slogan">Ye Olde Treasure Chest</h4>
    <ul class='list-unstyled'>
      {{# each }}
        <li>
          {{# link-to "product" this }}
          <img {{ bind-attr src=image }} height="50" width="50" class="avatar">
          {{ name }}
          {{/ link-to }}
          (${{ to_dollars price }})
        </li><br>
      {{/ each }}
    </ul>
  </div>
  <div class='col-lg-8'>
    {{ outlet }}
  </div>
</div>